{
	"bucketSubdomain": "wedding",
	"dnsDomain": "sebastiencoste.info",
	"sendReport": [
		{
			"type": "lambda",
			"destimation": "../../../Wedding/"
		}
	],
	"source": {
		"relativePath": "../../../Wedding/",
		"pathOfBuiltFiles": "dist/",
		"prepareScript": {
			"filename": "configureDeploy.py",
			"arguments": [
				{
					"key": "--destinationFile",
					"value": "app/conf/lambdas.json"
				},
				{
					"key": "--lambdaReport",
					"value": "lambdaReport.json"
				}
			]
		},
		"buildCommand": "gulp build"
	},
	"bucketPermissions": {
		"ACL": "public-read"
	},
	"bucketHosting": {
		"routingRules": [
			{
				"Redirect": {
					"HostName": "<bucketName>",
					"Protocol": "http",
					"ReplaceKeyPrefixWith": "#/"
				}
			}
		],
		"indexDocument": "index.html",
		"errorDocument": "index.html"
	},
	"bucketPolicy": {
		"Version": "2012-10-17",
		"Statement": [
			{
				"Sid": "PublicReadGetObject",
				"Effect": "Allow",
				"Principal": {
					"AWS": "*"
				},
				"Action": "s3:GetObject",
				"Resource": "arn:aws:s3:::<bucketName>/*"
			}
		]
	},
	"corsConfiguration": {
		"CORSRules": [
			{
				"AllowedMethods": [
					"GET",
					"PUT"
				],
				"ExposeHeaders": [
					"GET",
					"PUT"
				],
				"AllowedOrigins": [
					"*"
				],
				"MaxAgeSeconds": 3000,
				"AllowedHeaders": [
					"Authorization"
				]
			}
		]
	}
}